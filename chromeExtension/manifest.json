{
	"manifest_version" : 3,
	"name" : "MyExtension",
	"version" : "1.0.0",
	"description" : "This is my first extension",
	"background" : {
		"service_worker" : "Programs/background.js"
	},
	"permissions" : [
		"activeTab",
		"tabs"
	],
	"content_scripts" : [
		{
			"matches" : [
				"https://*/*",
				"http://*/*"
			],
			"js" : [
				"Programs/content.js"
			],
			"runAt" : "document_end"
		}
	]
}
